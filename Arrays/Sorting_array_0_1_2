// Array of only enteries 0 1 2 and sort it 


// Brute method 
 Use Merge sort

// Better method
 #include<iostream>
 int main(){
   int n ;
   int a[n];
   int cut0= 0, cut1=1, cut2=2;
   for (int  i = 0; i < n; i++)
   {
     if (a[i]==0) cut0++;
     else if(a[i]==1)cut1++;
     else cut2++; 
   }
   for(int i=0;i<cut0;i++ ) a[i]=0;
   for(int i=0;i<cut0+cut1;i++) a[i]=1;
   for(int i=cut0+cut1;i<n;i++) a[i]=2;

   
 }
