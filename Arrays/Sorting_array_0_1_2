// Array of only enteries 0 1 2 and sort it 


// Brute method 
 Use Merge sort

// Better method
 #include<iostream>
 int main(){
   int n ;
   int a[n];
   int cut0= 0, cut1=1, cut2=2;
   for (int  i = 0; i < n; i++)
   {
     if (a[i]==0) cut0++;
     else if(a[i]==1)cut1++;
     else cut2++; 
   }
   for(int i=0;i<cut0;i++ ) a[i]=0;
   for(int i=0;i<cut0+cut1;i++) a[i]=1;
   for(int i=cut0+cut1;i<n;i++) a[i]=2;

   
 }

// Optimal Solution

// Using dutch National Flag algorithm
// it states that everything from 0 to low-1 will be 0 which is extreme right
// and low to mid-1 will be 1
// and high+1 to n-1 will be 2  and this is extreme left


#include<stdio.h>
int main(){
    int n;
    int low = 0,mid =0,high = n-1;

    while (mid<=high)
    {
       if (arr[mid]==0)
       {
        swap(arr[low],arr[mid]);
        low++;
        mid++;
       }

       else if(arr[mid]==1){
         mid++;
       }

       else{
     swap(arr[mid],arr[high])
     high--;
       }
       
    }
    

}
